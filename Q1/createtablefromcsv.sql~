CREATE TABLE IF NOT EXISTS Relationtable (FieldID INT,
										Filename VARCHAR(20),
										Filter VARCHAR(20),
										MJD DOUBLE,
										CONSTRAINT fk1 FOREIGN KEY (FieldID, MJD) REFERENCES Stars1H(FieldID, MJD),
										CONSTRAINT fk2 FOREIGN KEY (FieldID, MJD) REFERENCES Stars1J(FieldID, MJD),
										CONSTRAINT fk3 FOREIGN KEY (FieldID, MJD) REFERENCES Stars1K1(FieldID, MJD),
										CONSTRAINT fk4 FOREIGN KEY (FieldID, MJD) REFERENCES Stars1K2(FieldID, MJD),
										CONSTRAINT fk5 FOREIGN KEY (FieldID, MJD) REFERENCES Stars1K3(FieldID, MJD),
										CONSTRAINT fk6 FOREIGN KEY (FieldID, MJD) REFERENCES Stars1Y(FieldID, MJD),
										CONSTRAINT fk7 FOREIGN KEY (FieldID, MJD) REFERENCES Stars1H(FieldID, MJD),
										CONSTRAINT fk8 FOREIGN KEY (FieldID, MJD) REFERENCES Stars1H(FieldID, MJD),
										CONSTRAINT fk9 FOREIGN KEY (FieldID, MJD) REFERENCES Stars1H(FieldID, MJD),
										CONSTRAINT fk10 FOREIGN KEY (FieldID, MJD) REFERENCES Stars1H(FieldID, MJD),
										CONSTRAINT fk11 FOREIGN KEY (FieldID, MJD) REFERENCES Stars1H(FieldID, MJD),
										CONSTRAINT fk12 FOREIGN KEY (FieldID, MJD) REFERENCES Stars1H(FieldID, MJD),
										CONSTRAINT fk13 FOREIGN KEY (FieldID, MJD) REFERENCES Stars1H(FieldID, MJD),
										CONSTRAINT fk14 FOREIGN KEY (FieldID, MJD) REFERENCES Stars1H(FieldID, MJD),
										CONSTRAINT fk15 FOREIGN KEY (FieldID, MJD) REFERENCES Stars1H(FieldID, MJD),
										CONSTRAINT fk16 FOREIGN KEY (FieldID, MJD) REFERENCES Stars1H(FieldID, MJD),
										CONSTRAINT fk17 FOREIGN KEY (FieldID, MJD) REFERENCES Stars1H(FieldID, MJD),
										CONSTRAINT fk18 FOREIGN KEY (FieldID, MJD) REFERENCES Stars1H(FieldID, MJD)
										);
.mode csv
.import tabledatalocationq1.csv Relationtable

