CREATE TABLE IF NOT EXISTS Stars1H (RunningID DOUBLE,
									Ra DOUBLE,
									Dec DOUBLE,
									X DOUBLE,
									Y DOUBLE,
									Flux1 DOUBLE,
									dFlux1 DOUBLE,
									Flux2 DOUBLE,
									dFlux2 DOUBLE,
									Flux3 DOUBLE,
									dFlux3 DOUBLE,
									Class INT,
									Mag1 DOUBLE,
									dMag1 DOUBLE,
									Mag2 DOUBLE,
									dMag2 DOUBLE,
									Mag3 DOUBLE,
									dMag3 DOUBLE,
									StarID INT,
									MJD DOUBLE,
									FieldID INT
									);

